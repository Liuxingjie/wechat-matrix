page{
  background: #F6F6F6;
}
.contain{
  width:100%;
  height:auto;
}
.header_img{
  width:100%;
  height:320rpx;
}
.titleBox{
  width:100%;
  height: auto;
  color:#000000;
  font-size: 28rpx;
  line-height: 27rpx;
  padding:38rpx 42rpx;
  background:#ffffff;
}
.timer{
  color:#B4BAC6;
  font-size: 24rpx;
  line-height: 23rpx;
  margin-top: 18rpx;
}
.com-box{
  padding:50rpx 70rpx;
  background: #ffffff;
  margin-top: 20rpx;
}
.list-item{
  height:100%;
  display: inline-block;
  text-align: center;
}
.list-item .nav-title{
  font-size: 28rpx;
  margin-top: 15rpx;
}
.list-item image{
  width:54rpx;
  height:54rpx;
}

.btns{
  width:100%;
  padding:0 45rpx 40rpx 45rpx ;
  height:120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background:#ffffff;
}
.btns .btn{
  width:calc(50% - 10rpx);
  height:80rpx;
}