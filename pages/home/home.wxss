
.com-page{
  width:100%;
  height:100%;
}
.searchBox{
  padding:0 35rpx;
  margin-top: 16rpx;
  margin-bottom: 30rpx;
}
.homeBody{
  padding:0 35rpx;
}
.swiper{
  width:100%;
  height:310rpx;
}
.swiper-item image{
  width:100%;
  height:100%;
}
.list_item{
  width:50%;
  height:auto;
  display: inline-block;
  padding-right: 20rpx;
  margin-bottom:40rpx;
}
.list_item:nth-of-type(2n){
  padding-right: 0;
  padding-left: 20rpx;
}
.flex-column{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.list_item .title{
  font-size: 29rpx;
  line-height:27rpx;
  color:#393939;
  font-weight: bold;
  margin-bottom:17rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis
}
.list_item .personNumber{
  font-size: 25rpx;
  line-height:23rpx;
  color: #757575;
}
.com-title{
  margin:29rpx 0;
}
.primary-title{ 
  font-size: 31rpx;
  line-height: 29rpx;
  font-weight: bold;
}
.second-title{
  font-size: 21rpx;
  line-height: 29rpx;
  color: #393939;
  margin-left: 18rpx;
}
