@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

button::after{
  border:none;
}
button{
  padding:0 !important;
  display:inline-block;
  background-color:transparent;
  box-sizing:initial;
  border:none;
}
.fix:after {
  content: " ";
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
  line-height: 0;
}
